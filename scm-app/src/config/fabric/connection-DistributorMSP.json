{
    "name": "lettuce-scm-app",
    "version": "1.0.0",
    "organization": {
        "DistributorOrg": {
            "mspid": "DistributorMSP",
            "peers": [
                "peer0.distributor.example.com"
            ],
            "certificateAuthorities": [
                "ca-distributor"
            ]
        }
    },
    "peers": {
        "peer0.distributor.example.com": {
            "url": "grpc://localhost:10051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.distributor.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/distributor.example.com/tlsca/tlsca.distributor.example.com-cert.pem"
            }
        }
    },
    "certificateAuthorities": {
        "ca-distributor": {
            "url": "http://localhost:10054",
            "caName": "ca-distributor",
            "httpOptions": {
                "verify": false
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/distributor.example.com/ca/ca.distributor.example.com-cert.pem"
            }
        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "grpc://localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/ordererOrganizations/example.com/ca/ca.example.com-cert.pem"
            }
        }
    }
}