{
    "name": "lettuce-scm-app",
    "version": "1.0.0",
    "organizations": {
        "RetailerOrg": {
            "mspid": "RetailerMSP",
            "peers": [
                "peer0.retailer.example.com"
            ]
        }
    },
    "peers": {
        "peer0.retailer.example.com": {
            "url": "grpc://localhost:11051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.retailer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/retailer.example.com/tlsca/tlsca.retailer.example.com-cert.pem"
            }
        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "grpc://localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/ordererOrganizations/example.com/ca/ca.example.com-cert.pem"
            }
        }
    }
}