{
    "name": "lettuce-scm-app",
    "version": "1.0.0",
    "organization": {
        "FarmerOrg": {
            "mspid": "FarmerMSP",
            "peers": [
                "peer0.farmer.example.com"
            ],
            "certificateAuthorities": [
                "ca-farmer"
            ]
        }
    },
    "peers": {
        "peer0.farmer.example.com": {
            "url": "grpc://localhost:9051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.farmer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/farmer.example.com/tlsca/tlsca.farmer.example.com-cert.pem"
            }
        }
    },
    "certificateAuthorities": {
        "ca-farmer": {
            "url": "http://localhost:9054",
            "caName": "ca-farmer",
            "httpOptions": {
                "verify": false
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/farmer.example.com/ca/ca.farmer.example.com-cert.pem"
            }
        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "grpc://localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/ordererOrganizations/example.com/ca/ca.example.com-cert.pem"
            }
        }
    }
}