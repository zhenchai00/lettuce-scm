{
    "name": "lettuce-scm-app",
    "version": "1.0.0",
    "organization": {
        "AdminOrg": {
            "mspid": "AdminMSP",
            "peers": [
                "peer0.admin.example.com"
            ],
            "certificateAuthorities": [
                "ca-admin"
            ]
        }
    },
    "peers": {
        "peer0.admin.example.com": {
            "url": "grpc://localhost:8051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.admin.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/admin.example.com/tlsca/tlsca.admin.example.com-cert.pem"
            }
        }
    },
    "certificateAuthorities": {
        "ca-admin": {
            "url": "http://localhost:8054",
            "caName": "ca-admin",
            "httpOptions": {
                "verify": false
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/peerOrganizations/admin.example.com/ca/ca.admin.example.com-cert.pem"
            }
        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "grpc://localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.example.com"
            },
            "tlsCACerts": {
                "path": "../../../../fabric-network/crypto-config/ordererOrganizations/example.com/ca/ca.example.com-cert.pem"
            }
        }
    }
}